<%- include('layout/header_dssci_biometric') %>
    <div class="container">
        <div class="row">    
            <div class="col-lg-6 mx-auto mt-4">
                <div class="card-shadow">
                    <div class="card-header bg-dark">
                        <h6 class="text-light">Add DSSCI Biometrics</h6>
                    </div>
                    <div class="card-body p-4">
                        <form action="/add_dssci_biometric" method="post" id="add-form" enctype="multipart/form-data">
                            <div class="mb-3">
                                <label for="unitNumber">Unit Number</label>
                                <input type="text" name="unitNumber" class="form-control form-control-md" placeholder="Enter Unit Number" required />
                            </div>
                            <div class="mb-3">
                                <label for="brand">Name</label>
                                <input type="text" name="brand" class="form-control form-control-md" placeholder="Enter Name" required />
                            </div>
                            <div class="mb-3">
                                <label for="serialNumber">Serial Number</label>
                                <input type="text" name="serialNumber" class="form-control form-control-md" placeholder="Enter Serial Number" required />
                            </div>
                            <div class="mb-3">
                                <label for="tag">Tagging Serialization</label>
                                <input type="text" name="tag" class="form-control form-control-md" placeholder="Enter Tagging Serialization" required />
                            </div>
                            <div class="mb-3">
                                <label for="roomLocation">location</label>
                                <input type="text" name="roomLocation" class="form-control form-control-md" placeholder="Enter Location" required />
                            </div>
                            <div class="mb-3">
                                <label for="ip">IP Address</label>
                                <input type="text" name="ip" class="form-control form-control-md" placeholder="Enter IP Address" required />
                            </div>
                            <div class="mb-3">
                                <label for="image" class="form-label">Select Image</label>
                                <input class="form-control form-control-md" type="file" name="image" required />
                            </div>
                            <div class="mb-3 d-grid">
                                <input type="submit" name="submit" value="Add Biometrics" class="btn btn-dark btn-md" />
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
<%- include('layout/footer_dssci_biometric') %>
